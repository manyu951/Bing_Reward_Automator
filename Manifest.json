{
    "manifest_version":3,
    "name":"Bing Reward",
    "version":"1.0",
    "description":"Automatic search microsoft bing reward",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "background":{
        "service_worker":"background.js"    
    },
    "icons": { 
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "action":{
        "default_popup":"popup.html"
        
    }
    
}